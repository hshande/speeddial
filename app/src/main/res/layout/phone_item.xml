<?xml version="1.0" encoding="utf-8"?>
<com.sunday.speeddial.customview.SquareView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/click_rl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp">

    <ImageView
        android:id="@+id/phone_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:visibility="gone"></ImageView>

    <TextView
        android:id="@+id/phone_photo_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="true"
        android:background="@color/colorAccent"
        android:gravity="center_vertical|center_horizontal"
        android:text="0"
        android:textColor="@color/colorWhite"
        android:textSize="60sp"
        android:visibility="gone"></TextView>

</com.sunday.speeddial.customview.SquareView>
